{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "jsx": "preserve",
    "outDir": "dist",
    "target": "es2016",
    "module": "es2015",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "removeComments": false,
    "sourceMap": true,
    "skipLibCheck": true
  }
}
